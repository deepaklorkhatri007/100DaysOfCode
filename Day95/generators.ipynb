   "source": [
    "def countdown():\n",
    "    yield 3\n",
    "    yield 2\n",
    "    yield 1\n",
    "    yield 'Blast off!'\n",
    "    \n",
    "g = countdown()\n",
    "next(g)\n",
    "x = next(g)\n",
    "print(x)\n",
    "y, z = next(g), next(g)\n",
    "print(z)\n",
    "# next(g) will give StopIteration: error"
   ]
  }
